{
  "name": "html-layout-parser-node",
  "version": "0.0.1",
  "description": "HTML Layout Parser for Node.js environments - compiled to WebAssembly. Supports multi-font management, file-based font loading, and batch processing.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./wasm": "./dist/html_layout_parser.wasm",
    "./wasm-js": "./dist/html_layout_parser.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && pnpm run copy:wasm",
    "copy:wasm": "cp ../../wasm-output/html_layout_parser.wasm dist/ && cp ../../wasm-output/html_layout_parser.js dist/",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "html",
    "layout",
    "parser",
    "wasm",
    "webassembly",
    "node",
    "nodejs",
    "server",
    "text-rendering",
    "typography",
    "batch-processing"
  ],
  "author": "maomao123 <nwyzx126@126.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tajigaqzh/html-layout-parser.git"
  },
  "bugs": {
    "url": "https://github.com/Tajigaqzh/html-layout-parser/issues"
  },
  "homepage": "https://Tajigaqzh.github.io/html-layout-parser/",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "sideEffects": false
}
