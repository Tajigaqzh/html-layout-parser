{
  "name": "html-layout-parser",
  "version": "0.0.1",
  "description": "High-performance HTML layout parser compiled to WebAssembly. Supports multi-font management, CSS separation, and multiple output modes for Canvas rendering.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "web": ["./dist/web.d.ts"],
      "worker": ["./dist/worker.d.ts"],
      "node": ["./dist/node.d.ts"]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/web.d.ts",
        "default": "./dist/web.js"
      },
      "require": {
        "types": "./dist/web.d.cts",
        "default": "./dist/web.cjs"
      }
    },
    "./worker": {
      "import": {
        "types": "./dist/worker.d.ts",
        "default": "./dist/worker.js"
      },
      "require": {
        "types": "./dist/worker.d.cts",
        "default": "./dist/worker.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./wasm": "./dist/html_layout_parser.wasm",
    "./wasm-js": "./dist/html_layout_parser.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && pnpm run copy:wasm",
    "copy:wasm": "cp ../../wasm-output/html_layout_parser.wasm dist/ && cp ../../wasm-output/html_layout_parser.js dist/",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "html",
    "layout",
    "parser",
    "wasm",
    "webassembly",
    "canvas",
    "text-rendering",
    "typography",
    "font",
    "css",
    "dom"
  ],
  "author": "maomao123 <nwyzx126@126.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tajigaqzh/html-layout-parser.git"
  },
  "bugs": {
    "url": "https://github.com/Tajigaqzh/html-layout-parser/issues"
  },
  "homepage": "https://Tajigaqzh.github.io/html-layout-parser/",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "sideEffects": false
}
